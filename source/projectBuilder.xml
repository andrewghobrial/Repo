<?xml version="1.0" encoding="UTF-8"?>
<project name="Guava.makejar" default="makejar" basedir=".">
  <target name ="makejar" description="Create a jar for the Guava project">
      <javac srcdir="."
      destdir="../build"
      classpath="guava-14.0.1.jar"
      debug="on"
      source="1.4"
      />
    <jar jarfile="out.jar" includes="*.class" basedir="../build"/>
  </target>
</project>